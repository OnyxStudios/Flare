{
  "expo": {
    "name": "Flare",
    "slug": "Flare",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/onyx/flare/assets/images/logo.png",
    "splash": {
      "image": "./src/onyx/flare/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
	  "bundleIdentifier": "onyx.flare",
      "supportsTablet": true
    },
	"android": {
	  "package": "onyx.flare"
	},
	"notification": {
		"iosDisplayInForeground": true
	}
  }
}
